    "\n",
    "        \"\"\"\n",
    "        cls.path_docs = path_docs\n",
    "        all_matches = []\n",
    "        print('file merge to get a single file...') \n",
    "        for path in tqdm(cls.get_file_list(path_docs)):\n",
    "            df = pd.read_csv(Path(path_docs) / Path(path))\n",
    "            if  'PLAYER_ID' not in df.columns:\n",
    "                continue\n",
    "            df = df.fillna(0)[cls.TARGET_COLUMNS]\n",
    "            df['MIN'] = df['MIN'].apply(min_to_second)\n",
    "            all_matches.append(df)\n",
    "            \n",
    "        all_matches_info = pd.concat(all_matches, axis=0).reset_index(drop=True)\n",
    "        return cls(all_matches_info, path_docs)\n",
    "            \n",
